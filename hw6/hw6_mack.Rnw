sig.fn <- function(dmat, model, sill, nugget, range, offset, sigma.vec, location){
  Sigma <- cov.spatial(dmat, cov.model = model,
                       cov.pars=c(sill - nugget, range/offset))
  pred <- c(rep(1,7))
Sigma.star <- data.frame(cbind(Sigma,pred))
pred <- c(rep(1,7),0)
Sigma.star[8,] <- c(pred)

# find sigma star
