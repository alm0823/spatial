sigma.star <- t(as.matrix(sigma.star))

lambda.star <- sigma.star %*% solve(Sigma.star) 

pred.new <- sum(lambda.star[-8]*location[,3])
var.new <- sill - nugget - sum(lambda.star*sigma.star)

return(c(pred.new, var.new, c(lambda.star)[-8]))
}

